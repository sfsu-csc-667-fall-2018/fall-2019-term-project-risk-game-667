{
  "name": "risk-game-667",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development REDIS_URL=5000 DEBUG=APP_NAME:* nodemon --ignore public ./bin/www",
    "start:win": "SET \"NODE_ENV=development\" SET DEBUG=myapp:* & nodemon ./bin/www",
    "build": "webpack --config webpack.config.js",
    "build:dev": "webpack --config webpack.config.js -w",
    "clean": "rm ./public/javascripts/bundles/*.js",
    "db:create:migration": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo:all",
    "db:heroku:test": "node ./db/heroku-test",
    "postinstall": "sequelize db:migrate",
    "format": "prettier --write bin/www '**/*.js'",
    "slides:gen": "npx @marp-team/marp-cli -o docs/index.html docs/slides.md",
    "slides:serve": "npx serve docs"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^4.0.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.4.4",
    "pug": "^2.0.4",
    "redis": "^3.0.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "socket.io": "^2.4.0",
    "three": "^0.116.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "nodemon": "^2.0.2",
    "prettier": "2.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
